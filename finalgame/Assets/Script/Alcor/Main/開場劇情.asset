  eachChapter:
  - eachPart_Sentences:
    - speaker: "\u5976\u9165\u59B9"
      sentence:
      - "\u54C8\u56C9\u5C0F\u7CBE\u9748!\u6211\u662F\u5976\u9165\u59B9"
      - "\u6211\u7684\u8001\u5976\u5976\u4F4F\u5728\u68EE\u6797\u88E1\uFF0C\u6211\u8981\u51FA\u9580\u53BB\u770B\u6211\u7684\u8001\u5976\u5976\u56C9"
      - "\u68EE\u6797\u88E1\u6709\u6211\u7684\u597D\u670B\u53CB\u5154\u5B50\u3001\u677E\u9F20\u3001\u6885\u82B1\u9E7F\u548C\u53F0\u7063\u9ED1\u718A"
      - "\u6211\u6703\u9806\u9053\u53BB\u770B\u770B\u6211\u597D\u670B\u53CB\uFF0C\u4F60\u4E5F\u4E00\u8D77\u4F86\u5427!"
    - speaker: "\u5C0F\u7CBE\u9748"
      sentence:
      - "\u597D\u5594! \u842C\u4E00\u5728\u78B0\u5230\u68EE\u6797\u88E1\u7684\u8A0E\u53AD\u7684\u72D0\u72F8\u3001\u8001\u9DF9\u3001\u5927\u91CE\u72FC\u548C\u7375\u4EBA"
      - "\u4ED6\u5011\u5C31\u6703\u51FA\u4E00\u4E9B\u96E3\u984C\u4F86\u70BA\u96E3\u59B3"
      - "\u53EA\u8981\u4F60\u6709\u6E05\u695A\u7684\u982D\u8166\uFF0C\u901A\u904E\u95DC\u5361"
      - "\u6211\u5C31\u6703\u5E6B\u59B3\u8D95\u8D70\u4ED6\u5011\uFF0C\u4E0D\u8B93\u4ED6\u5011\u7E7C\u7E8C\u70BA\u96E3\u59B3\u5537"
      - "\u89E3\u6551\u670B\u53CB\u4E4B\u5F8C\uFF0C\u59B3\u7684\u597D\u670B\u53CB\u5011\u9084\u6703\u4E3B\u52D5\u7F8E\u5473\u7684\u98DF\u6750\u505A\u70BA\u7DAD\u6301\u68EE\u6797\u548C\u5E73\u7684\u734E\u52F5\u5594~"
      - "\u9019\u6A23\u5230\u9054\u5976\u5976\u5BB6\u7684\u6642\u5019\u59B3\u5C31\u6709\u98DF\u6750\u53EF\u4EE5\u505A\u6210\u7F8E\u98DF\u7D66\u5976\u5976\u5927\u5FEB\u6735\u9824\u56C9~"
    - speaker: "\u5976\u9165\u59B9"
      sentence:
      - "\u597D!\u51FA\u767C\u5427!"
