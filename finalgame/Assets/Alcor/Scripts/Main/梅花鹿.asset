  m_Script: {fileID: 11500000, guid: 8adb408064a65cf43a37809d5bca09a0, type: 3}
  m_Name: "\u6885\u82B1\u9E7F"
  m_EditorClassIdentifier: 
  ID: 0
  IndexPart: 0
  eachChapter:
  - eachPart_Sentences:
    - speaker: "\u5927\u91CE\u72FC"
      sentence:
      - "\u563F\u563F\u563F\uFF0C\u5C0F\u9E7F\u770B\u8D77\u4F86\u771F\u662F\u8B93\u4EBA\u5782\u6D8E\u6B32\u6EF4\u963F\uFF0C\u5982\u6B64\u7F8E\u5473\u7121\u6BD4\u7684\u98DF\u7269\u4E0D\u597D\u597D\u7684\u4EAB\u53D7\u5C31\u592A\u53EF\u60DC\u4E86\u2026 "
    - speaker: "\u5976\u9165\u59B9"
      sentence:
      - "\u5927\u91CE\u72FC\uFF0C\u597D\u4E45\u4E0D\u898B\uFF0C\u4F60\u5728\u9019\u88E1\u9B3C\u9B3C\u795F\u795F\u7684\u5230\u5E95\u8981\u505A\u4EC0\u9EBC\u5462?\u662F\u4E0D\u662F\u53C8\u8981\u5E79\u58DE\u4E8B\u554A?"
    - speaker: "\u5927\u91CE\u72FC"
      sentence:
      - "\u4F60\u53EB\u90A3\u9EBC\u5927\u8072\u505A\u4EC0\u9EBC?\uFF0C\u4F60\u770B\u6211\u7684\u597D\u4E8B\u90FD\u88AB\u4F60\u7834\u58DE\u4E86\uFF0C\u53EF\u60E1!"
    - speaker: "\u5976\u9165\u59B9"
      sentence:
      - "\u54C8\u54C8\u54C8\uFF0C\u5C0D\u4E0D\u8D77\u5537\uFF0C\u6211\u4E0D\u77E5\u9053\u4F60\u6B63\u5728\u505A\u4EC0\u9EBC\u597D\u4E8B\uFF0C\u5C31\u662F\u770B\u4F60\u8EB2\u8EB2\u85CF\u85CF\uFF0C\u6211\u4EE5\u70BA\u4F60\u6B63\u5728\u73A9\u8EB2\u8C93\u8C93\u5462~"
    - speaker: "\u5927\u91CE\u72FC"
      sentence:
      - "\u53EF\u60E1!\u4F60\u6C23\u6B7B\u6211\u4E86\uFF0C\u6211\u4E0D\u7BA1\uFF0C\u5976\u9165\u59B9\u4F60\u628A\u6211\u76EF\u8457\u534A\u5929\u7684\u7F8E\u98DF\u5F04\u4E1F\u4E86\uFF0C\u4F60\u5F97\u8CE0\u6211\u2026"
    - speaker: "\u5976\u9165\u59B9"
      sentence:
      - "\u597D\u5427! \u5927\u91CE\u72FC\uFF0C\u4F60\u4E0A\u6B21\u95DC\u5361\u88AB\u6211\u7834\u4E86\uFF0C\u807D\u4F60\u53C8\u6709\u65B0\u628A\u6232\uFF0C\u6211\u5C31\u63A5\u53D7\u4F60\u7684\u6311\u6230\uFF0C\u51FA\u984C\u5427!"
      - "\u82E5\u6211\u6210\u529F\u4E86\uFF0C\u4ECA\u5929\u4F60\u5C31\u96E2\u53BB\u5427!
        \u82E5\u6211\u8F38\u4E86\uFF0C\u6211\u5C31\u8ACB\u4F60\u5403\u9B54\u6CD5\u5976\u5976\u505A\u7684\u7F8E\u5473\u7684\u98DF\u7269\u7D66\u4F60\uFF0C\u600E\u9EBC\u6A23\u554A?"
    - speaker: "\u5927\u91CE\u72FC"
      sentence:
      - "\u54C8\u54C8\u54C8! \u6211\u8A2D\u8A08\u7684\u65B0\u95DC\u5361\uFF0C\u4F60\u5225\u60F3\u904E\u95DC"
      - "\u4F60\u6700\u597D\u73FE\u5728\u5C31\u8B93\u5C0F\u7CBE\u9748\u65BD\u9B54\u6CD5\u6C42\u5976\u5976\uFF0C\u8D95\u5FEB\u5F04\u500B\u7F8E\u98DF\uFF0C\u6211\u53EF\u662F\u5F88\u4E86\u2026 "
    - speaker: "\u5C0F\u7CBE\u9748"
      sentence:
      - "\u54CE\u5440! \u4F60\u5011\u5169\u500B\u5225\u5EE2\u8A71\u4E86\uFF0C\u5C31\u8B93\u6211\u53EC\u559A\u5927\u91CE\u72FC\u7684\u95DC\u5361\u51FA\u4F86\uFF0C\u8A66\u8A66\u770B\u4FBF\u77E5\u3002"
  - eachPart_Sentences:
    - speaker: "\u5976\u9165\u59B9"
      sentence:
      - "\u5927\u91CE\u72FC\uFF0C\u4F60\u9019\u6B21\u7684\u95DC\u5361\u679C\u7136\u5F88\u53B2\u5BB3\uFF0C\u6211\u8A66\u4E86\u5F88\u591A\u65B9\u6CD5\u624D\u89E3\u958B\u984C\u76EE\u3002\u4F46\u4E0D\u7BA1\u5982\u4F55\uFF0C\u6211\u9084\u662F\u8D0F\u4E86\u3002"
    - speaker: "\u5927\u91CE\u72FC"
      sentence:
      - "\u54FC! \u4E0B\u6B21\u4F60\u53EF\u80FD\u5C31\u6C92\u6709\u9019\u9EBC\u5E78\u904B\u4E86\u3002\u54FC!\u4EE5\u5F8C\u6700\u597D\u9084\u662F\u4E0D\u8981\u96A8\u4FBF\u7834\u58DE\u6211\u7684\u597D\u4E8B\u2026\u518D\u898B! "
    - speaker: "\u5976\u9165\u59B9"
      sentence:
      - "\u54CE\u5440! \u4F60\u9019\u6A23\u8AAA\u5C31\u50B7\u611F\u60C5\u5566! \u6211\u600E\u9EBC\u77E5\u9053\u4F60\u751A\u9EBC\u6642\u5019\u8981\u505A\u597D\u4E8B\uFF0C\u6709\u7DE3\u6211\u5011\u61C9\u8A72\u9084\u662F\u6703\u898B\u9762\u7684\u3002"
    - speaker: "\u5C0F\u7CBE\u9748"
      sentence:
      - "\u5927\u91CE\u72FC\u90FD\u8D70\u4E86\uFF0C\u6211\u5011\u4E5F\u8D95\u5FEB\u8D95\u8DEF\u5427!"
    - speaker: "\u6885\u82B1\u9E7F"
      sentence:
      - "\u9165\u59B9\uFF0C\u771F\u7684\u592A\u611F\u6FC0\u59B3\u4E86\uFF0C\u8981\u4E0D\u662F\u4F60\u53CA\u6642\u51FA\u8072\uFF0C\u6211\u5011\u90FD\u6C92\u6709\u767C\u73FE\u5927\u91CE\u72FC\u5C31\u5728\u6211\u5011\u8EAB\u908A"
      - "\u771F\u662F\u5687\u6B7B\u6211\u5011\u4E86\uFF0C\u6211\u7684\u5E7C\u5BB0\u9E7F\u5E78\u8667\u6709\u4F60\u624D\u4E0D\u81F3\u65BC\u8B8A\u6210\u5927\u91CE\u72FC\u7684\u76E4\u4E2D\u98E7\uFF0C\u771F\u662F\u592A\u611F\u6FC0\u59B3\u4E86\u3002 "
      - "\u4F60\u8DDF\u5C0F\u7CBE\u9748\u9019\u662F\u8981\u53BB\u54EA\u88E1\u5440!
        \u6211\u525B\u525B\u807D\u5230\u5C0F\u7CBE\u9748\u8AAA\uFF0C\u4F60\u5011\u8981\u8D95\u8DEF\u554A? "
    - speaker: "\u5976\u9165\u59B9"
      sentence:
      - "\u6211\u5011\u78BA\u5BE6\u662F\u8981\u8D95\u8DEF\u7684\uFF0C\u6211\u8981\u53BB\u5976\u5976\u5BB6\uFF0C\u518D\u9072\u4E86\u6015\u662F\u8D95\u4E0D\u4E0A\u8DDF\u5976\u5976\u4E00\u8D77\u904E\u5E74!"
    - speaker: "\u6885\u82B1\u9E7F\u5ABD\u5ABD"
      sentence:
      - "\u9019\u6A23\u5440! \u70BA\u4E86\u611F\u6FC0\u4F60\uFF0C\u6211\u8B93\u9E7F\u7238\u7238\u7528\u96EA\u6A47\u9001\u4F60\u5011\u4E00\u7A0B\u5427!
        \u9E7F\u7238\u7238\u8DD1\u5F97\u5FEB\uFF0C\u4F60\u5750\u96EA\u6A47\u6BD4\u4F60\u722C\u5C71\u6D89\u6C34\u8981\u5FEB\u5F97\u591A\u5427!"
    - speaker: "\u5976\u9165\u59B9"
      sentence:
      - "\u54CE\u5440! \u9019\u600E\u9EBC\u597D\u610F\u601D\u5440! \u4E0D\u904E\u9E7F\u7238\u7238\u82E5\u80FD\u9001\u6211\u4E00\u7A0B\uFF0C\u78BA\u5BE6\u53EF\u4EE5\u8B93\u6211\u7BC0\u7701\u4E0D\u5C11\u6642\u9593\u5462~"
    - speaker: "\u6885\u82B1\u9E7F\u5ABD\u5ABD"
      sentence:
      - "\u4E0D\u7528\u5BA2\u6C23\uFF0C\u4F60\u7684\u6551\u547D\u5927\u6069\uFF0C\u6211\u5011\u6B63\u6101\u7121\u4EE5\u70BA\u5831\u5462~"
      - "\u73FE\u5728\u5C31\u51FA\u767C\u5427! \u82E5\u9047\u5230\u66B4\u96EA\uFF0C\u4F60\u5011\u9084\u5F97\u82B1\u6642\u9593\u8EB2\u4E00\u8EB2\u5462~
        \u61C9\u8A72\u6C92\u8FA6\u6CD5\u4E00\u76F4\u8D95\u8DEF\u2026 "
