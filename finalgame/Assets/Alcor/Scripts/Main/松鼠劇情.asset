  m_Script: {fileID: 11500000, guid: 8adb408064a65cf43a37809d5bca09a0, type: 3}
  m_Name: "\u677E\u9F20\u5287\u60C5"
  m_EditorClassIdentifier: 
  ID: 0
  IndexPart: 0
  eachChapter:
  - eachPart_Sentences:
    - speaker: "\u5976\u9165\u59B9"
      sentence:
      - "\u7591~\u767C\u751F\u751A\u9EBC\u4E8B\u4E86\u2026 \u9019\u8072\u97F3\u807D\u8C48\u4E0D\u592A\u5999\u963F~"
    - speaker: "\u5C0F\u7CBE\u9748"
      sentence:
      - "\u770B\u4F86\u662F\u6709\u71B1\u9B27\u53EF\u4EE5\u77A7\u77A7\u4E86\uFF0C\u6211\u5011\u904E\u53BB\u770B\u770B\u5427! "
    - speaker: "\u5976\u9165\u59B9"
      sentence:
      - "\u4F60\u9019\u96BB\u7A7A\u4E2D\u7684\u60E1\u9738\uFF0C\u4E0D\u5728\u7A7A\u4E2D\u8000\u6B66\u63DA\u5A01\uFF0C\u70BA\u4EC0\u9EBC\u8DD1\u5230\u9019\u88E1\u4F86\u6B3A\u4FAE\u6211\u7684\u670B\u53CB\u5C0F\u677E\u9F20\uFF0C\u4F60\u771F\u662F\u6B3A\u4EBA\u592A\u751A\u2026 "
    - speaker: "\u8001\u9DF9"
      sentence:
      - "\u54FC! \u6709\u4F60\u5011\u751A\u9EBC\u4E8B\uFF0C\u7ADF\u6562\u7BA1\u8001\u5B50\u7684\u9592\u4E8B\u3002"
      - " \u5C0F\u7CBE\u9748\u5225\u4EE5\u70BA\u6211\u6015\u4F60\uFF0C\u8981\u4E0D\u662F\u4F60\u6709\u9B54\u6CD5\u68D2\uFF0C\u96E3\u9053\u4F60\u9084\u6703\u662F\u6211\u7684\u5C0D\u624B\uFF0C\u54FC! "
    - speaker: "\u5976\u9165\u59B9"
      sentence:
      - "\u5225\u5EE2\u8A71\u8001\u898F\u77E9\uFF0C\u4F60\u51FA\u984C\u5427! \u8981\u662F\u6211\u80FD\u904E\u95DC\uFF0C\u4F60\u5C31\u5F97\u653E\u904E\u5C0F\u677E\u9F20"
    - speaker: "\u8001\u9DF9"
      sentence:
      - "\u54C8\u54C8\u54C8\u2026\u6C92\u554F\u984C\uFF0C\u8001\u5B50\u7B49\u4F60\u5F88\u4E45\u4E86"
      - "\u4F60\u9019\u500B\u81EA\u4EE5\u70BA\u662F\u7684\u50A2\u4F19\uFF0C\u82E5\u662F\u4F60\u89E3\u4E0D\u51FA\u4F86\uFF0C\u4EE5\u5F8C\u770B\u5230\u8001\u5B50\u6211\uFF0C\u4F60\u5C31\u5F97\u7E5E\u8DEF\u8D70\uFF0C\u518D\u4E5F\u4E0D\u8A31\u591A\u7BA1\u6211\u7684\u9592\u4E8B\u2026\u807D\u5230\u4E86\u6C92\u6709\u2026 "
    - speaker: "\u5C0F\u7CBE\u9748"
      sentence:
      - " \u597D\uFF0C\u6211\u5011\u5C31\u4F86\u770B\u770B\u8001\u9DF9\u8A2D\u8A08\u4E86\u751A\u9EBC\u96E3\u95DC\u2026"
  - eachPart_Sentences:
    - speaker: "\u5C0F\u7CBE\u9748"
      sentence:
      - "\u65BD\u9B54\u6CD5\u62C9"
    - speaker: "\u8001\u9DF9"
      sentence:
      - "\u54FC\uFF01\u7B97\u4F60\u597D\u904B\uFF0C\u6211\u767C\u8A93\u4E00\u5B9A\u6703\u518D\u60F3\u51FA\u66F4\u96E3\u7684\u95DC\u5361\uFF0C\u5230\u6642\u5019\u6211\u4E00\u5B9A\u8981\u770B\u54ED\u8457\u96E2\u958B\u2026\u54FC! "
    - speaker: "\u5976\u9165\u59B9"
      sentence:
      - "\u597D\u96AA\uFF0C\u6211\u525B\u525B\u5DEE\u4E00\u9EDE\u89E3\u4E0D\u51FA\u4F86\u5462~"
    - speaker: "\u5C0F\u677E\u9F20"
      sentence:
      - "\u5976\u9165\u59B9\u5BE6\u5728\u592A\u611F\u6FC0\u4F60\u4E86\uFF0C\u9084\u597D\u4F60\u53CA\u6642\u8D95\u5230\uFF0C\u4E0D\u7136\u6211\u9019\u689D\u5C0F\u547D\u5C31\u6C92\u4E86"
    - speaker: "\u5976\u9165\u59B9"
      sentence:
      - "\u6C92\u4E8B\u4E86\uFF0C\u5225\u5BB3\u6015\uFF0C\u8001\u9DF9\u98DB\u8D70\u4E86\uFF0C\u77ED\u6642\u9593\u61C9\u8A72\u4E0D\u6703\u518D\u4F86\u4E86\uFF0C\u7260\u5F97\u9589\u95DC\u82E6\u7DF4\uFF0C\u624D\u80FD\u51FA\u66F4\u96E3\u7684\u95DC\u5361\u4F86\u70BA\u96E3\u6211\u5462\u2026"
    - speaker: "\u5C0F\u677E\u9F20"
      sentence:
      - "\u8B1D\u8B1D\u4F60\uFF0C\u5976\u9165\u59B9\u4F60\u600E\u9EBC\u6703\u51FA\u73FE\u5728\u9019\u88E1? "
    - speaker: "\u5976\u9165\u59B9"
      sentence:
      - "\u6211\u8981\u53BB\u63A2\u671B\u5976\u5976\uFF0C\u525B\u597D\u8DEF\u904E\u9019\u88E1\uFF0C\u807D\u5230\u8001\u9DF9\u9577\u562F\uFF0C\u6211\u5C31\u77E5\u9053\u6709\u554F\u984C\uFF0C\u6240\u4EE5\u8D95\u7DCA\u904E\u4F86\u770B\u770B"
    - speaker: "\u5C0F\u677E\u9F20"
      sentence:
      - "\u771F\u7684\u592A\u611F\u8B1D\u4E86\uFF0C\u65E2\u7136\u4F60\u8981\u53BB\u5976\u5976\u5BB6\uFF0C\u6211\u9019\u88E1\u525B\u597D\u6709\u4E00\u4E9B\u860B\u679C\uFF0C\u525B\u525B\u63A1\u4E0B\u4F86\u7684\uFF0C\u4F60\u62FF\u53BB\u8DDF\u5976\u5976\u4E00\u8D77\u5403\u5427!"
    - speaker: "\u5976\u9165\u59B9"
      sentence:
      - " \u592A\u597D\u4E86\uFF0C\u5976\u5976\u6700\u559C\u6B61\u5403\u860B\u679C\u6D3E\u4E86\uFF0C\u6211\u8981\u5E36\u53BB\u5976\u5976\u5BB6\u505A\u860B\u679C\u6D3E\u7D66\u5976\u5976\u5403\u2026"
    - speaker: "\u5C0F\u7CBE\u9748"
      sentence:
      - "\u54C8\u54C8\u592A\u68D2\u4E86\u2026\u6211\u4F86\u5E6B\u4F60\u628A\u860B\u679C\u6536\u4E0B\u56C9~"
      - " \u597D\u4E86\uFF0C\u6211\u5011\u8981\u7E7C\u7E8C\u51FA\u767C\u56C9~ \u4E0D\u7136\u5C31\u4F86\u4E0D\u53CA\u8DDF\u5976\u5976\u4E00\u8D77\u904E\u65B0\u5E74\u4E86\u2026"
