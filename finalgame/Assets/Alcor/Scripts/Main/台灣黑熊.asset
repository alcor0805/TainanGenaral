  m_Script: {fileID: 11500000, guid: 8adb408064a65cf43a37809d5bca09a0, type: 3}
  m_Name: "\u53F0\u7063\u9ED1\u718A"
  m_EditorClassIdentifier: 
  ID: 0
  IndexPart: 0
  eachChapter:
  - eachPart_Sentences:
    - speaker: "\u5976\u9165\u59B9"
      sentence:
      - "\u9019\u4F4D\u5927\u53D4\uFF0C\u9019\u9EBC\u51B7\u7684\u5929\u6C23\u9084\u51FA\u4F86\u7375\u98DF\u963F!"
    - speaker: "\u7375\u4EBA"
      sentence:
      - "\u662F\u554A! \u5BD2\u51AC\u72E9\u7375\u5927\u578B\u52D5\u7269\uFF0C\u6B63\u662F\u597D\u6642\u6A5F\u3002"
    - speaker: "\u5976\u9165\u59B9"
      sentence:
      - "\u5509! \u5E0C\u671B\u6211\u7684\u670B\u53CB\u5011\u90FD\u80FD\u8EB2\u5F97\u904E\u4F60\u7121\u60C5\u7684\u5B50\u5F48\uFF0C\u6211\u53EF\u898B\u4E0D\u5F97\u6211\u7684\u670B\u53CB\u5011\u88AB\u4F60\u7375\u6BBA\u2026"
    - speaker: "\u5976\u9165\u59B9"
      sentence:
      - "\u7B49\u4E00\u4E0B\uFF0C\u7375\u4EBA\u5927\u53D4\uFF0C\u4F60\u53EF\u4EE5\u4E0D\u53EF\u4EE5\u653E\u904E\u9019\u96BB\u9ED1\u718A\uFF0C\u4ED6\u662F\u6211\u7684\u597D\u670B\u53CB\uFF0C\u4ED6\u4E0D\u6703\u50B7\u5BB3\u6211\u5011\uFF0C\u5E0C\u671B\u4F60\u624B\u4E0B\u7559\u60C5\u2026"
    - speaker: "\u7375\u4EBA"
      sentence:
      - "\u9019\u6A23\u554A! \u4F60\u78BA\u5B9A\u9019\u96BB\u9ED1\u718A\u4E0D\u6703\u50B7\u4EBA\u2026"
    - speaker: "\u5976\u9165\u59B9"
      sentence:
      - "\u6211\u53EF\u4EE5\u4FDD\u8B49\u9ED1\u718A\u4E0D\u6703\u50B7\u4EBA\u2026"
      - "\u9019\u6A23\u5427!\u6309\u7167\u9019\u500B\u68EE\u6797\u88E1\u7684\u898F\u77E9\uFF0C\u4F60\u51FA\u500B\u984C\u5427!\u82E5\u6211\u80FD\u89E3\u958B\uFF0C\u4F60\u5C31\u5FC5\u9808\u653E\u9ED1\u718A\u8D70\uFF0C\u4E0D\u53EF\u4EE5\u50B7\u5BB3\u7260\u2026"
      - "\u82E5\u662F\u6211\u8F38\u4E86\uFF0C\u6211\u5C31\u8CA0\u8CAC\u716E\u4E00\u9910\u7F8E\u5473\u7684\u4F73\u991A\u6B3E\u5F85\u4F60\uFF0C\u5982\u4F55\u554A? "
    - speaker: "\u7375\u4EBA"
      sentence:
      - "\u597D!\u6C92\u554F\u984C\uFF0C\u807D\u8AAA\u80FD\u5403\u5230\u5976\u9165\u59B9\u505A\u7684\u7F8E\u98DF\uFF0C\u5728\u9019\u68EE\u6797\u88E1\u53EF\u662F\u6975\u70BA\u7A00\u7F55\u7684\u4E8B"
      - "\u8981\u9EBB\u7169\u5C0F\u7CBE\u9748\u5E6B\u6211\u53EC\u559A\u6211\u8A2D\u8A08\u7684\u95DC\u5361\u2026 "
    - speaker: "\u5C0F\u7CBE\u9748"
      sentence:
      - " \u6C92\u554F\u984C"
  - eachPart_Sentences:
    - speaker: "\u5976\u9165\u59B9"
      sentence:
      - "\u6211\u8D0F\u4E86!"
    - speaker: "\u7375\u4EBA"
      sentence:
      - "\u5509! \u96E3\u602A\u68EE\u6797\u88E1\u6C92\u5E7E\u500B\u4EBA\u5403\u904E\u50B3\u8AAA\u4E2D\u7684\u9B54\u6CD5\u5976\u5976\u7684\u7F8E\u98DF\uFF0C\u5728\u4E0B\u662F\u8877\u5FC3\u7684\u4F69\u670D\uFF0C\u770B\u4F86\u9019\u6B21\u6C92\u53E3\u798F\u4E86"
      - "\u73FE\u5728\u5916\u9762\u7684\u98A8\u96EA\u4E5F\u8B8A\u5C0F\u4E86\uFF0C\u6211\u4E5F\u8981\u56DE\u53BB\uFF0C\u4E0B\u6B21\u518D\u898B\u4E86\u5594!"
    - speaker: "\u5976\u9165\u59B9"
      sentence:
      - "\u597D\uFF0C\u518D\u898B\uFF0C\u6162\u8D70\u5594~ "
    - speaker: "\u9ED1\u718A"
      sentence:
      - "\u5976\u9165\u59B9\uFF0C\u8B1D\u8B1D\u4F60\uFF0C\u9019\u662F\u6211\u525B\u525B\u624D\u51FA\u53BB\u5916\u9762\u63A1\u4E0B\u4F86\u7684\u8702\u871C\uFF0C\u9001\u7D66\u4F60\u3002"
    - speaker: "\u5976\u9165\u59B9"
      sentence:
      - "\u592A\u597D\u4E86\uFF0C\u6211\u6B63\u597D\u8981\u53BB\u770B\u5976\u5976\uFF0C\u5976\u5976\u6700\u559C\u6B61\u559D\u9999\u751C\u7684\u8702\u871C\u6AB8\u6AAC\u6C41\u4E86. "
    - speaker: "\u5C0F\u7CBE\u9748"
      sentence:
      - "\u54C8\u54C8\u592A\u68D2\u4E86\u6211\u4E5F\u559C\u6B61\u559D\u8702\u871C\u6AB8\u6AAC\u6C41\u2026\u6211\u4F86\u5E6B\u4F60\u628A\u8702\u871C\u6536\u4E0B\u56C9~
        \u5728\u87A2\u5E55\u53F3\u4E0A\u89D2\u51FA\u73FE\u79AE\u7269\u8702\u871C "
      - " \u597D\u4E86\uFF0C\u5976\u5976\u5BB6\u5FEB\u5230\u4E86\uFF0C\u8D81\u98A8\u96EA\u4E0D\u5927\uFF0C\u6211\u5011\u8D95\u5FEB\u53BB\u627E\u5976\u5976\u5427! "
